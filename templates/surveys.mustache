<div class="card">
<div class="card-body">
<h5 class="card-title" >Encuestas</h5>
<table class="table table-bordered">
    <thead>
        <tr id="{{id}}">
            <th>NOMBRE ENCUESTA</th>
            <th>FECHA INICIO</th>
            <th>FECHA FIN</th>
            <th>REQUERIDA</th>
            <th>ESTADO</th>
        </tr>
    </thead>
    <tbody>
        {{#surveys}}
        <tr id='{{id}}'>
            <td>{{name}}</td>
            <td>{{startdate}}</td>
            <td>{{enddate}}</td>
            <td>{{required}}</td>
            <td>{{status}}</td>
        </tr>
        {{/surveys}}
    </tbody>
</table>
</div>
</div>